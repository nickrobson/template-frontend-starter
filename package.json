{
  "private": true,
  "name": "template-frontend-starter",
  "version": "1.0.0",
  "description": "A template for quickly bootstrapping frontends",
  "author": "Nick Robson",
  "license": "MIT",
  "browserslist": "> 1%, last 2 versions, not dead",
  "source": "entry/src/index.html",
  "workspaces": [
    "entry",
    "packages/*"
  ],
  "scripts": {
    "start": "parcel",
    "build": "rm -rf dist && parcel build",
    "prepare": "husky install",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "test": "jest"
  },
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "@compiled/react": "^0.10.4",
    "@swc/helpers": "^0.3.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@compiled/babel-plugin": "^0.15.0",
    "@compiled/eslint-plugin": "^0.4.1",
    "@parcel/core": "^2.4.1",
    "@parcel/validator-typescript": "^2.4.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@yarnpkg/types": "^4.0.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-runner-eslint": "^1.0.0",
    "lint-staged": "^12.3.7",
    "parcel": "^2.4.1",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "optionalDependencies": {
    "node-notifier": "^10.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
